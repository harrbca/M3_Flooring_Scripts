{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": ["warn"]
  },
  "globals": {
    "H5ControllerBase": "readonly",
    "H5": "readonly",
    "ScriptUtil": "readonly",
    "controller": "readonly",
    "applicationController": "readonly"
  }
}